{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/meaty-products/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/meaty-products/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/meaty-products",
  "projects": {
    "meaty-products": {
      "type": "application",
      "root": "apps/meaty-products",
      "entryFile": "main",
      "sourceRoot": "apps/meaty-products/src",
      "compilerOptions": {
        "tsConfigPath": "apps/meaty-products/tsconfig.app.json"
      }
    },
    "upload": {
      "type": "application",
      "root": "apps/upload",
      "entryFile": "main",
      "sourceRoot": "apps/upload/src",
      "compilerOptions": {
        "tsConfigPath": "apps/upload/tsconfig.app.json"
      }
    }
  }
}